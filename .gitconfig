[user]
  name = Aleksandr Gnatenko
  email = alexgnat@gmail.com
[push]
  default = simple
[credential]
  helper = osxkeychain
[core]
  pager = less -FRSX
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  editor = vim
  autocrlf = input
  safecrlf = true
  excludesfile = /Users/aleksandr-gest/.gitignore_global
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[color "branch"]
  current = green reverse
  local   = black
  remote  = red
[alias]
  l = log --all --graph --pretty='format:%C(yellow)%h %C(green)%an%C(reset) %C(blue)%ad%C(reset) %s %C(auto)%d%C(reset)' --date=format:'%d-%m-%Y %H:%I'